<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <!-- Recently Viewed Products Carousel -->
    <t t-name="rental_ecommerce.productsRecentlyViewed">
        <div t-att-id="uniqueId" class="carousel slide o_not_editable" data-interval="false">
            <div class="carousel-inner">
                <form id="rent_form" action="/website_form/" method="post" enctype="multipart/form-data" data-model_name="crm.lead" data-success-mode="redirect" data-success-page="/contactus-thank-you" class="o_mark_required" data-mark="*" data-editable-form="false">
                    <div class="row pt-4">
                        <div class="col-xs-12 col-sm-12 col-md-1 col-lg-1 col-xl-1"
                        style="padding: 0 !important; margin: 0 !important;">
                            <p style="padding-top: 8px; padding-bottom: 8px; text-align:center;">Date</p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5 col-xl-5"
                             style="padding: 0 !important; margin: 0 !important;">
                            <input
                                class="form-control"
                                name="pickup_date"
                                type="date"
                                autocomplete="off"
                                required="1"

                               />

                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-1 col-lg-1 col-xl-1"
                        style="padding: 0 !important; margin: 0 !important;">
                            <p style="padding-top: 8px; padding-bottom: 8px; text-align:center;">to</p>
                        </div>
                        <div class="col-xs-10 col-sm-10 col-md-4 col-lg-4 col-xl-4"
                             style="padding: 0 !important; margin: 0 !important;">
                            <input

                                class="form-control"
                                name="return_date"
                                type="date"
                                autocomplete="off"
                                required="1"

                              />
                        </div>
                    </div>
                     <div class="row pt-4">
                         <div class="col-xs-12 col-sm-12 col-md-1 col-lg-1 col-xl-1"
                            style="padding: 0 !important; margin: 0 !important;">
                            <p style="padding-top: 8px; padding-bottom: 8px; text-align:center;">Quantity</p>
                        </div>
                         <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5 col-xl-5"
                             style="padding: 0 !important; margin: 0 !important;">
                            <input
                                class="form-control"
                                name="quantity"
                                type="number"
                                autocomplete="off"
                                required="1"
                                min="1"
                              />
                        </div>
                         <div class="col-xs-12 col-sm-12 col-md-1 col-lg-1 col-xl-1"
                            style="padding: 0 !important; margin: 0 !important;">
                            <p style="padding-top: 8px; padding-bottom: 8px; text-align:center;">Duration</p>
                        </div>
                         <div class="col-xs-10 col-sm-10 col-md-4 col-lg-4 col-xl-4"
                             style="padding: 0 !important; margin: 0 !important;">
                            <input
                                class="form-control"
                                name="duration"
                                type="number"
                                autocomplete="off"
                                required="1"
                                min="1"
                              />
                        </div>
                         <div class="col-xs-2 col-sm-2 col-md-1 col-lg-1 col-xl-1"
                            style="padding: 0 !important; margin: 0 !important;">
                            <p style="padding-top: 8px; padding-bottom: 8px; text-align:center;">/days</p>
                        </div>

                     </div>
                    <div class="row pt-4">
                        <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 col-xl-3"
                             style="padding: 0 !important; margin-left: -28px;">
                            <p style="padding-top: 8px; padding-bottom: 8px; text-align:center;">Additional Items required?</p>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8 col-xl-8"
                             style="padding: 0 !important; margin-left: 26px;">
                            <input
                                class="form-control"
                                name="additional"
                                type="text"
                                autocomplete="off"


                              />
                        </div>


                    </div>
                    <div class="form-group col-12 s_website_form_submit" data-name="Submit Button">
                        <div style="width: 200px;" class="s_website_form_label"/>
                        <a href="#" role="button" class="btn btn-primary btn-lg s_website_form_send">Submit</a>
                        <span id="s_website_form_result"></span>
                    </div>
                </form>
            </div>

        </div>
    </t>
</templates>
